  <template name="EnableDisableCheck" value="function EnableDisableCheck(obj){&#10;&#9;var chks = document.getElementsByName('chkbox[]');&#10;&#9;if(obj.checked){&#10;&#9;&#9;for (var i=0;i&lt;chks.length;i++){&#10;&#9;&#9;&#9;chks[i].disabled = true;&#10;&#9;&#9;}&#10;&#9;}else{&#10;&#9;&#9;for (var i=0;i&lt;chks.length;i++){&#10;&#9;&#9;&#9;chks[i].disabled = false;&#10;&#9;&#9;}&#10;&#9;}&#10;}" description="EnableDisableCheck(obj)" toReformat="true" toShortenFQNames="true">
