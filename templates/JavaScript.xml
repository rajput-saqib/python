  <template name="getRadioValue" value="function getRadioValue(paytype)&#10;{&#10;for(i=0;i&lt;document.escrow[paytype].length;i++)&#10;{&#10;if (document.escrow[paytype][i].checked == true)&#10;return document.escrow[paytype][i].value;&#10;}&#10;}" description="getRadioValue(paytype)" toReformat="true" toShortenFQNames="true">
