  <template name="select_all" value="function select_all(obj){&#10;&#9;var chks = document.getElementsByName('chkbox[]');&#10;&#9;if(obj.checked){&#10;&#9;&#9;for (var i=0;i&lt;chks.length;i++){&#10;&#9;&#9;&#9;chks[i].checked = true;&#10;&#9;&#9;}&#10;&#9;}else{&#10;&#9;&#9;for (var i=0;i&lt;chks.length;i++){&#10;&#9;&#9;&#9;chks[i].checked = false;&#10;&#9;&#9;}&#10;&#9;}&#10;}" description="select all Chkbox" toReformat="true" toShortenFQNames="true">
